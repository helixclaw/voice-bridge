{
  "name": "voice-bridge",
  "version": "1.0.0",
  "description": "Modular voice-to-AI-to-voice bridge",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:watch": "vitest",
    "deploy-commands": "node dist/transports/discord/deploy-commands.js"
  },
  "keywords": [
    "voice",
    "ai",
    "discord",
    "tts",
    "stt"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.29.3",
  "pnpm": {
    "onlyBuiltDependencies": [
      "sodium-native"
    ]
  },
  "dependencies": {
    "@discordjs/voice": "^0.19.0",
    "@snazzah/davey": "^0.1.9",
    "discord.js": "^14.25.1",
    "opusscript": "^0.1.1",
    "prism-media": "^1.3.5",
    "sodium-native": "^5.0.10"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}